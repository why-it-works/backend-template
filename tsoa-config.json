{
	"entryFile": "src/index.ts",
	"noImplicitAdditionalProperties": "throw-on-extras",
	"controllerPathGlobs": ["src/controllers/*.ts"],
	"spec": {
		"outputDirectory": "src",
		"specVersion": 3,
		"basePath": "/",
		"specFileBaseName": "swagger"
	},
	"routes": {
		"routesDir": "src/routes",
		"middleware": "express",
		"authenticationModule": "./src/middleware/auth"
	}
}
